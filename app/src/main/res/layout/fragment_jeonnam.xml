            android:gravity="center"
            android:backgroundTint="@color/white"
            android:textColor="@color/black"/>

        <Button
            android:id="@+id/btnJeonnamWando"
            android:layout_width="120dp"
            android:layout_height="50dp"
            android:text="완도군"
            android:textSize="13dp"
