    <RelativeLayout
        android:id="@+id/map_view"
        android:layout_width="match_parent"
        android:layout_height="600dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </RelativeLayout>

    <ImageButton
        android:id="@+id/btnCosCreate"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:layout_marginLeft="24dp"
        android:layout_marginBottom="40dp"
        android:background="@color/white"
        android:scaleType="fitCenter"
        android:src="@drawable/btn_course_create"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:id="@+id/btnCosRecommend"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:background="@color/white"
        android:scaleType="fitCenter"
        android:src="@drawable/btn_course_recommend"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toEndOf="@+id/btnCosCreate"
        app:layout_constraintTop_toTopOf="@+id/btnCosCreate" />


</androidx.constraintlayout.widget.ConstraintLayout>

